        Button.TextColor3 = Color3.new(0.756863, 0.756863, 0.756863)
        Button.TextSize = 37
        Button.TextWrapped = true
 
        Button:MouseButton1Down:Connect(function()
            pcall(callback)
        end)
    end
 
    return Win
 
end
 
return Library
