    local Win = {}
 
    function Win:Button(name, callback)
        local callback = callback or function() end
        local Button = Instance.new("TextButton")
 
        Button.Name = "Button"
        Button.Parent = TabContentContainer
        Button.BackgroundColor3 = Color3.new(0.34902, 0.34902, 0.34902)
        Button.Size = UDim2.new(0, 517, 0, 55)
